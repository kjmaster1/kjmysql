{
  "name": "kjmysql",
  "author": "kjmaster1",
  "version": "3.0.0",
  "description": "A FiveM MySQL resource using node-mysql2",
  "type": "module",
  "main": "dist/build.js",
  "scripts": {
    "build": "node build.js",
    "watch": "node build.js --watch",
    "postinstall": "patch-package",
    "build:all": "pnpm recursive run build",
    "release": "pnpm run build:all && node build.js --release"
  },
  "dependencies": {
    "mysql2": "3.11.3",
    "named-placeholders": "1.1.3",
    "node-cache": "^5.1.2",
    "node-fetch": "3.3.2"
  },
  "devDependencies": {
    "@citizenfx/server": "^2.0.5132-1",
    "@types/node": "^22.7.4",
    "@types/node-cache": "^4.2.5",
    "archiver": "^7.0.1",
    "esbuild": "^0.21.5",
    "patch-package": "^8.0.1",
    "typescript": "^5.6.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "svelte-preprocess"
    ]
  }
}